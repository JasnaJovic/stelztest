  <script type="text/javascript">
  var Shopify = Shopify || {};
  // ---------------------------------------------------------------------------
  // Money format handler
  // ---------------------------------------------------------------------------
  {% raw %}
    Shopify.money_format = "${{amount}}";
  {% endraw %}
  Shopify.formatMoney = function(cents, format) {
      if (typeof cents == 'string') {
          cents = cents.replace('.', '');
      }
      var value = '';
      var placeholderRegex = /\{\{\s*(\w+)\s*\}\}/;
      var formatString = (format || this.money_format);
  
      function defaultOption(opt, def) {
          return (typeof opt == 'undefined' ? def : opt);
      }
  
      function formatWithDelimiters(number, precision, thousands, decimal) {
          precision = defaultOption(precision, 2);
          thousands = defaultOption(thousands, ',');
          decimal = defaultOption(decimal, '.');
  
          if (isNaN(number) || number == null) {
              return 0;
          }
  
          number = (number / 100.0).toFixed(precision);
  
          var parts = number.split('.'),
              dollars = parts[0].replace(/(\d)(?=(\d\d\d)+(?!\d))/g, '$1' + thousands),
              cents = parts[1] ? (decimal + parts[1]) : '';
  
          return dollars + cents;
      }
      switch (formatString.match(placeholderRegex)[1]) {
          case 'amount':
              value = formatWithDelimiters(cents, 2);
              break;
          case 'amount_no_decimals':
              value = formatWithDelimiters(cents, 0);
              break;
          case 'amount_with_comma_separator':
              value = formatWithDelimiters(cents, 2, '.', ',');
              break;
          case 'amount_no_decimals_with_comma_separator':
              value = formatWithDelimiters(cents, 0, '.', ',');
              break;
      }
  
      return formatString.replace(placeholderRegex, value);
  };
  
  var c_format = document.getElementsByTagName('body')[0].getAttribute('data-money-format');
  var pro_option = document.getElementsByClassName('.pro_options');
  for (var i = 0; i < pro_option.length; i++) {
      var e = pro_option[i].querySelector('[name="selling_plan"]');
      var sub_discount = e.options[e.selectedIndex].getAttribute('data-discount');
      pro_option[i].querySelector('.discount_text').innerHTML = 'save ' + sub_discount + '%';
  }
  
  $(function () {
    $(`[name='quantity'],[name="option-1"],[name="option-2"],[name="option-3"],[name="subscribe"],[name="product"],[name="selling_plan_allocation"]`).on('change',function(){
      adjustPricingAndOptions();
    }); 
    adjustPricingAndOptions();
  })
  
  
  function adjustPricingAndOptions(){
    let subscription_type = $(".shopify-product-form .pro_options.visible").find(`[name="subscribe"]:checked`).val();
    let option_1 = $(".shopify-product-form .pro_options.visible").find(`[name="option-1"]:checked`).val();
    let is_option_2 = false;
    let is_option_3 = false;
    let variant_title = '';
    let quantity = parseInt($(".shopify-product-form .pro_options.visible").find(`[name="quantity"]`).val());
    variant_title += option_1;
  
    if($(".shopify-product-form .pro_options.visible").find(`[name="option-2"]`).length > 0) {
      is_option_2 = true;
      let option_2 = $(".shopify-product-form .pro_options.visible").find(`[name="option-2"]:checked`).val();
      variant_title += ` / ${option_2}`;
    }
  
    if($(".shopify-product-form .pro_options.visible").find(`[name="option-3"]`).length > 0) {
      is_option_3 = true;
      let option_3 = $(".shopify-product-form .pro_options.visible").find(`[name="option-3"]:checked`).val();
      variant_title += ` / ${option_3}`;
    }
  
    let variant_ids = $(".shopify-product-form .pro_options.visible").find(`[name="id"]`).find('option');
    let variant_selected_em = '';
    $(variant_ids).removeAttr('selected');
    $(variant_ids).each(function(ix,em){
      let em_variant_title = $(em).text().toString().trim();
      if(em_variant_title == variant_title) {
        $(em).attr('selected','selected');
        variant_selected_em = $(em);
      }
    });
  
    item_price = $(variant_selected_em).attr('data-price');
    if(subscription_type == 'One-time purchase') {
      item_price = $(variant_selected_em).attr('data-price');
    } else {
      let selling_plan_option = $(".shopify-product-form .pro_options.visible").find(`[name="selling_plan_allocation"]:checked`).val();
      item_price = $(".shopify-product-form .pro_options.visible").find(`[name="selling_plan"] option[value="${selling_plan_option}"][data-var-title='${variant_title}']`).attr('data-var-price');
      $(".shopify-product-form .pro_options.visible").find(`[name="selling_plan"] option`).removeAttr('selected');
      $(".shopify-product-form .pro_options.visible").find(`[name="selling_plan"] option[value="${selling_plan_option}"][data-var-title='${variant_title}']`).attr('selected','selected');
    }
    let total_price = quantity * parseInt(item_price);
    let price_formatted = Shopify.formatMoney(item_price, c_format);
    let total_price_formatted = Shopify.formatMoney(total_price, c_format);
    $(".shopify-product-form .pro_options.visible").find(`#pPrice`).text(total_price_formatted);
  }
  
  function subChange(evt) {
      var $this = evt;
      var val = $this.parentNode.parentNode.querySelector('input[name="subscribe"]:checked').value;
  
      if (val == 'One-time purchase') {
          $this.parentNode.parentNode.querySelector('.selling_plan_allocation').style.display = 'none';
          $this.parentNode.parentNode.querySelector('.selling_plans option').removeAttribute('selected');
          $this.parentNode.parentNode.querySelector('.selling_plans').removeAttribute('name');
      } else if (val == 'Subscribe') {
          $this.parentNode.parentNode.querySelector('.selling_plan_allocation').style.display = 'flex';
          $this.parentNode.parentNode.querySelector('.selling_plans').setAttribute('name', 'selling_plan');
          $this.parentNode.parentNode.querySelector('.selling_plan_allocation input+label').click();
  
          var dis_option = $this.parentNode.parentNode.querySelector('[name="selling_plan"]');
          var sub_discount = dis_option.options[dis_option.selectedIndex].getAttribute('data-discount');
          $this.parentNode.parentNode.querySelector('.discount_text').innerHTML = 'save ' + sub_discount + '%';
  
      }
  }
  
  
  function flavoursChange(ele) {
      var price = ele.getAttribute('data-price');
      var title = ele.getAttribute('data-title');
      var sub_img = document.getElementsByClassName("sub_img");
      var mix_pack_header = document.getElementsByClassName("mix_pack_header");
      
      var elems = document.querySelectorAll(".pro_options");
      for (var i = 0; i < elems.length; i += 1) {
          var t = elems[i].getAttribute('data-pro-title');
          if (t == title) {
              elems[i].style.display = 'block';
              $(elems[i]).addClass('visible');
          } else {
              elems[i].style.display = 'none';
              $(elems[i]).removeClass('visible');
          }
      }
      
      document.querySelector('.pro_options input[name="subscribe"]').removeAttribute('checked');
      document.querySelector('.pro_options[data-pro-title="' + title + '"] input[name="subscribe"][value="One-time purchase"]+label').click();
    
      for(let i=0; i < sub_img.length; i++){
        if(sub_img[i].getAttribute("data-title") == title){
          sub_img[i].classList.add("active_sub_image");
        } else{
          sub_img[i].classList.remove("active_sub_image");
        }
    
    }
     for(let j=0; j < mix_pack_header.length; j++){
        if(mix_pack_header[j].getAttribute("data-title") == title){
          mix_pack_header[j].classList.add("mix_pack_header_active");
        } else{
          mix_pack_header[j].classList.remove("mix_pack_header_active");
        }  
    }
  }
  </script>
