<style>
    .mix_pack_header {
      position: absolute;
      font-size: 8vw;
      color: rgb(var(--color-foreground));
      line-height: 1;
      top: -8%;
      right: -100%;
      width: 50vw;
      text-align: center;
      font-family: PlantinStd-BoldCondensed;
      display: none;
      text-transform: uppercase;
    }
    .mix_pack_header_active {
      display: block;
    }
    .subscriptionProduct .grid {
      margin-left: 0;
  }
    .mix_pack_wave {
       position: absolute;
       width: 100vw;
       top: 50%;
       left: calc(-5vw - 5rem);
       transform: translateY(-50%);
       height: auto;
       z-index: -10;
    }
   
    
    .can_move .active_sub_image_1 {
     
      animation: anima 3s ease-in-out 0s infinite;
    }
    .can_move .active_sub_image_2{
      
      animation: anima 3s ease-in-out 0.5s infinite;
    }
    .can_move .active_sub_image_3{
     
      animation: anima 3s ease-in-out 1.5s infinite;
    }
    .can_move .active_sub_image_4{
     
      animation: anima 3s ease-in-out 1s infinite;
    }
     @keyframes anima {
       0%   {transform: translatey(0);}
       50%  {transform: translatey(1.5vw);}
       100% {transform: translatey(0);}
     }
     
    .subscriptionProduct__left {
      position: relative;
      z-index: 5;
    }
    .index .mix_pack_images {
      position: relative;
      height: 25vw;
      width: 46vw;
    }
    .mix_pack_images {
      position: relative;
      height: 25vw;
      width: 28vw;
    }
    .sub_img{
     position: absolute;
      height: 100%;
      width: auto;
      transition: all 0.3s ease-in-out;
      display: none;
    }
    .active_sub_image {
      display: block;
    }
    .active_sub_image_1 {
  
      left: 0; 
      z-index: 37;
    }
    .active_sub_image_2{
  
      left: 25%;  
      z-index: 39;
    }
    .active_sub_image_3{
  
      left: 50%;
      z-index: 40;
    }
    .active_sub_image_4{
  
      left: 75%;
      z-index: 38;
    }
    .mix_pack_cont_sticky .sub_img {
     top: 0;
    }
    
    .mix_pack_cont {   
      z-index: 40;
    }
    .index .mix_pack_cont_sticky {
      position: fixed;
      top: 50vh;
       right: -40%;
    }
    .index .subscriptionProduct {
      padding-top: 25vw;
      padding-bottom: 10vw;
    }
    .subscriptionProduct {
      padding-top: 17.4vw;
      padding-bottom: 5vw;
    }
    #jankooo{
      height: 25vw;
    }
    
    .subs_section_price {
      display: block;
      margin: max(1.3vw,19px) 0;
      color: rgb(var(--color-foreground));
      font-family: 'Helsinki-Book';
      font-size: max(1.8vw,26px);
      line-height: 1;
    }
    #jankooo {
      height: 25vw;
      align-self: baseline;
      margin-top: 4.5vw;
    }
    .subscription_details{
      padding-bottom: max(0.7vw, 10px);
      position: relative;
      margin: 0;
      width: fit-content;
      display: flex;
      align-items: flex-end;
  }
    
    .subscription_details:hover .subscription_details--popup {
      display: block;
    }
    .subscription_details--label {
      margin: 0;
      color: rgb(var(--color-foreground));
      font-size: max(1vw, 14px);
      line-height: 1;
      font-family: 'Helsinki-Book';
    }
    .subscription_details--popup {
      position: absolute;
      top: max(1.7vw, 24px);
      background: rgb(var(--color-foreground));
      z-index: 99;
      padding: max(1.4vw, 20px) max(1vw, 20px);
      width: 26vw;
      display: none;
    }
    .subscription_details--popup::after {
    content: "";
    position: absolute;
    bottom: 100%;
    left: 0;
    border-width: max(0.5vw, 8px);
    border-style: solid;
    border-color: transparent transparent rgb(var(--color-foreground)) transparent;
  }
    .subscription_details--heading, .subscription_details--text, .subscription_details--link {
      color: rgb(var(--theme-switch-background));
        
    }
    .subscription_details--heading {
      font-family: 'Helsinki-Bold';
       margin: 0 0 max(1.4vw, 20px);
       font-size: max(1.25vw, 16px);
      line-height: 1;
    }
    .subscription_details--link {
      font-family: 'Helsinki-Book';
      text-decoration: none;
      font-size: max(1vw, 14px);
      line-height: 1;
    }
    .subscription_details--text {
      font-family: 'Helsinki-Light';
      margin: 0 0 max(1.4vw, 20px);
      font-size: max(1vw, 14px);
      line-height: 1;
    }
    .subscription_details--text p {
      margin: 0;
    }
    .reload-icon_subs {
      width: max(1.1vw, 16px);
      height: auto;
      margin-right: 10px;
    }
    @media only screen and (max-width: 989px) {
      .subscription_details--popup {
        width: 34vw;
      }
      .subscriptionProduct {
       padding-left: 0;
       padding-right: 0;
    }
      .mix_pack_wave {
      left: calc(-2.5vw - 2.5rem);
  
  }
      .subscriptionProduct .grid .sec_title {
        margin-top: 0;
      }
     
    
    }
    @media only screen and (max-width: 749px) {
      .index .mix_pack_cont_sticky {
       right: -63%;
    }
      .subscription_details--popup {
        width: 65vw;
      }
      .shopify-section-subscription-section .page-width{
          max-width: 100%;
      } 
      .subscriptionProduct {
         padding-top: 200px;
      }
      .index .subscriptionProduct {
         padding-top: 38vw;
      }
      #jankooo{
      height: 50vw;
      margin-bottom: 85px;
        margin-top: 0;
     }
      .mix_pack_images {
      height: 50vw;
      width: 46vw;
      margin-left: 19vw;
    }
      .index .mix_pack_images {
      height: 50vw;
      width: 92vw;
      margin-left: 19vw;
    }
      #subscriptionProduct__left {
        order: 2;
        height: fit-content;
      }
      .mix_pack_header {
        font-size: 13.6vw;
        top: calc(-85px - 63vw);
        right: 0;
        width: 100vw;
      }
      .subscriptionProduct__left {
        padding-left: 6vw !important;
        padding-right: 6vw;
      }
       .mix_pack_wave {
      position: absolute;
      width: 168vw;
      top: -19%;
      left: 50%;
      transform: translate(-50%,-50%);
      height: auto;
      z-index: -10;
      }
    }
  </style>
