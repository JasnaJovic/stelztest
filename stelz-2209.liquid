      <div id="mix_pack_cont" class="grid__item mix_pack_cont mix_pack_cont_sticky">
      <div id="mix_pack_images" class="mix_pack_images">
         
      {% for block in section.blocks %}
          {% assign index = forloop.index %}
          {% assign product = all_products[block.settings.product] %}
            {% for image in product.media offset:1 limit:4 %}
            <img  loading="lazy" width="auto" height="auto" data-title="{{ product.title | handle }}"  class="sub_img active_sub_image_{{ forloop.index }} {% if index == 1 %}active_sub_image{% endif %} " src="{% if image != blank %}{{ image | img_url: 'master' }}{% else %}{{ block.settings.pro_image | img_url: 'master' }}{% endif %}" alt="{% if image != blank %}{{ image.alt }}{% else %}{{ block.settings.pro_image.alt }}{% endif %}">
            {% endfor %}    
      {% endfor %}     
        
      </div>
