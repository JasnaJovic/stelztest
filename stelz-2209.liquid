  {% if template.name == 'index' %}
  <script>
       var mix_pack_cont = document.getElementById("mix_pack_cont");
       
       window.onscroll = function(){
        const {scrollTop, scrollHeight, clientHeight} = document.documentElement;
         var dest = document.getElementById("subscriptionProduct__left").offsetTop;
         var mix_pack_images = document.getElementById("mix_pack_images");
         var mix_pack_cont_sticky = document.getElementsByClassName("mix_pack_cont_sticky");
         var dest2 = document.getElementById("jankooo").offsetTop;
         var add_space = -25;
         
         if(window.screen.width < 750 ){
           dest = dest2;
         }
         
         var y = dest - 0.5*clientHeight;
         var d = (scrollTop)/y;
         var g = 46 - 32*(d - 0.5);
         
         if(window.screen.width < 750 ){
           g = 2*g;
            var z = -63 + 57*d*2;
           
         } else{
            add_space = screen.width/20;
           var z = -40 + 37*d*2;
         }
         console.log(add_space);
         
         if((scrollTop + 0.5*clientHeight) > (dest + add_space)){
            mix_pack_cont.classList.remove("mix_pack_cont_sticky");
            mix_pack_cont.classList.add("can_move");
           if(window.screen.width < 750 ){ 
             mix_pack_images.style.width = "56vw";
           }else {
             mix_pack_images.style.width = "28vw";
           }
                              
          }else {
            mix_pack_cont.classList.add("mix_pack_cont_sticky");
            mix_pack_cont.classList.remove("can_move");
            if(z<0) {
            mix_pack_cont_sticky[0].style.right = z + "%";
            } else {
               mix_pack_cont_sticky[0].style.right = "0";
            }
            if(d> 0.5){           
             mix_pack_images.style.width = g + "vw";
            }
          }
       };
       
  </script>
  
  {% endif %}
  
